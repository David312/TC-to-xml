<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="132.0" prefWidth="332.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.CreateSetLostCommTimer">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <children>
            <GridPane prefHeight="185.0" prefWidth="380.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints maxHeight="62.0" minHeight="10.0" prefHeight="44.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="94.0" minHeight="10.0" prefHeight="53.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="New Lost Comm Timer:" GridPane.rowIndex="1" />
                  <TextField fx:id="input" text="0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="Due Time:" />
                  <TextField fx:id="dueTime" text="0" GridPane.columnIndex="1" />
               </children>
            </GridPane>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="23.0" prefWidth="380.0" spacing="10.0">
               <children>
                  <Button fx:id="addBtn" mnemonicParsing="false" onAction="#handleAdd" text="Add TC" />
                  <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
